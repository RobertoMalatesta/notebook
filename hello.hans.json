	"cell6": {
		"input": {
			"type": "javascript",
			"source": "option = new EuropeanOption(\n  new PlainVanillaPayoff(Option.Type.Call, 100.0),\n  new EuropeanExercise(DateExt.UTC('7,June,2014'))\n)"
		},
		"show_editor": true,
		"output": {
			"type": "html",
			"content": "<div id=\"ot6\"><code>[object Object]</code></div>"
		}
